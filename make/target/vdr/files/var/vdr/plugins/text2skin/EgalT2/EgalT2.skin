  </display>
  
<!-- ReplaySmall-Display ======================================= -->

  <display id="replaySmall">
	<window x1="0" x2="-1" y1="0" y2="32" bpp="8"/>
	<window x1="0" x2="-1" y1="-225" y2="-1" bpp="8"/>

	<block x1="0" x2="90" y1="-180" y2="-36">
		<rectangle x1="0" x2="-1" y1="0" y2="-1" color="GraphDark"/>
		<ellipse x1="0" x2="4" y1="0" y2="4" color = "#00000000" arc="-2"/>
		<ellipse x1="-5" x2="-1" y1="0" y2="4" color = "#00000000" arc="-1"/>
		<ellipse x1="0" x2="4" y1="-5" y2="-1" color = "#00000000" arc="-3"/>
		<ellipse x1="-5" x2="-1" y1="-5" y2="-1" color = "#00000000" arc="-4"/>
		<!-- Mode, Title -->
		<text condition="equal({ReplayMode},'normal')" x1="10" x2="-10" y1="5" y2="30" color="TextActiv" align="center" font="Osd">REC</text>
		<text condition="equal({ReplayMode},'dvd')" x1="10" x2="-10" y1="5" y2="30" color="TextActiv" align="center" font="Osd">DVD</text>
		<text condition="not(or(equal({ReplayMode},'normal'),equal({ReplayMode},'dvd')))" x1="10" x2="80" y1="5" y2="30" color="TextActiv" align="center" font="Osd">{ReplayMode}</text>
		<rectangle x1="10" x2="-10" y1="33" y2="33" color="TextPassiv"/>
		<!-- playMode -->
		<image condition="{ReplayIsPlaying}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextPassiv" path="symbols/play.xpm"/>
		<image condition="{ReplayIsPausing}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/pause.xpm"/>
		<image condition="or({ReplayIsFastForward},{ReplayIsFastForward:0})" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd.xpm"/>
		<image condition="{ReplayIsFastForward:1}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd1.xpm"/>
		<image condition="{ReplayIsFastForward:2}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd2.xpm"/>
		<image condition="{ReplayIsFastForward:3}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd3.xpm"/>
		<image condition="{ReplayIsFastForward:4}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd4.xpm"/>
		<image condition="{ReplayIsFastForward:5}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd5.xpm"/>
		<image condition="{ReplayIsFastForward:6}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd6xpm"/>
		<image condition="{ReplayIsFastForward:7}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd7xpm"/>
		<image condition="{ReplayIsFastForward:8}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd8.xpm"/>
		<image condition="{ReplayIsFastForward:9}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/ffwd9.xpm"/>
		<image condition="or({ReplayIsSlowForward},{ReplayIsSlowForward:0})" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/sfwd.xpm"/>
		<image condition="{ReplayIsSlowForward:1}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/sfwd1.xpm"/>
		<image condition="{ReplayIsSlowForward:2}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/sfwd2.xpm"/>
		<image condition="{ReplayIsSlowForward:3}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/sfwd3.xpm"/>
		<image condition="or({IsFastRewind},{IsFastRewind:0})" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew.xpm"/>
		<image condition="{IsFastRewind:1}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew1.xpm"/>
		<image condition="{IsFastRewind:2}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew2.xpm"/>
		<image condition="{IsFastRewind:3}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew3.xpm"/>
		<image condition="{IsFastRewind:4}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew4.xpm"/>
		<image condition="{IsFastRewind:5}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew5.xpm"/>
		<image condition="{IsFastRewind:6}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew6.xpm"/>
		<image condition="{IsFastRewind:7}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew7.xpm"/>
		<image condition="{IsFastRewind:8}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew8.xpm"/>
		<image condition="{IsFastRewind:9}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/frew9.xpm"/>
		<image condition="or({IsSlowRewind},{IsSlowRewind:0})" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/srew.xpm"/>
		<image condition="{IsSlowRewind:1}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/srew1.xpm"/>
		<image condition="{IsSlowRewind:2}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/srew2.xpm"/>
		<image condition="{IsSlowRewind:3}" x1="30" x2="60" y1="58" y2="93" bgColor="GraphDark" color="TextActiv" path="symbols/srew3.xpm"/>
		<!-- Symbol Recording -->
		<rectangle x1="10" x2="-10" y1="-28" y2="-28" color="TextPassiv"/>
		<image condition="{IsRecording}" x="33" y="-23" bgColor="TextPassiv" path="symbols/recording.xpm"/>
	</block>

	<!-- Bottom: Buttons/Message -->
	<block condition="{Message}">
		<rectangle x1="0" x2="-1" y1="-35" y2="-1" color="GraphDark"/>
		<rectangle x1="0" x2="-1" y1="-34" y2="-34" color="TextPassiv"/>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgInfo" align="center" font="Osd">{MessageInfo}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgStat" align="center" font="Osd">{MessageStatus}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgWarn" align="center" font="Osd">{MessageWarning}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgErr" align="center" font="Osd">{MessageError}</marquee>
	</block>

  </display>

<!-- AudioTracks-Display =============================== -->
  
  <display id="audioTracks">
	<window x1="0" x2="-1" y1="-225" y2="-36" bpp="8"/>
	<window x1="0" x2="-1" y1="-35" y2="-1" bpp="8"/>

	<block x1="0" x2="200" y1="-208" y2="-36">
		<rectangle x1="0" x2="-1" y1="0" y2="-1" color="GraphDark"/>
		<ellipse x1="0" x2="6" y1="0" y2="6" color = "#00000000" arc="-2"/>
		<ellipse x1="-7" x2="-1" y1="0" y2="6" color = "#00000000" arc="-1"/>
		<ellipse x1="0" x2="6" y1="-7" y2="-1" color = "#00000000" arc="-3"/>
		<ellipse x1="-7" x2="-1" y1="-7" y2="-1" color = "#00000000" arc="-4"/>
		<text x1="10" x2="-10" y1="5" y2="30" color="TextActiv" font="Osd">{MenuTitle}</text>
		<rectangle x1="10" x2="-10" y1="33" y2="33" color="TextPassiv"/>
	<!-- text2skin todo: trans() not working @ audioTracks-Display
		<text condition="{AudioChannel}" x1="10" x2="145" y1="-27" y2="-1" color="TextPassiv" align="right" font="Sml">trans('Channel'):</text>
	-->
		<text condition="equal({AudioChannel},'stereo')" x1="150" x2="-10" y1="-27" y2="-1" color="TextPassiv" align="right" font="Sml">[LR]</text>
		<text condition="equal({AudioChannel},'left')" x1="150" x2="-10" y1="-27" y2="-1" color="TextPassiv" align="right" font="Sml">[L-]</text>
		<text condition="equal({AudioChannel},'right')" x1="150" x2="-10" y1="-27" y2="-1" color="TextPassiv" align="right" font="Sml">[-R]</text>
	</block>
	<!-- TrackList -->
	<list x1="5" x2="195" y1="-164" y2="-71">
		<item height="24"/>
		<text x1="30" x2="-5" y1="0" y2="23" color="TextPassiv" font="Sml">{MenuItem}</text>
		<image condition="{IsMenuCurrent}" x="5" y="0" color="TextActiv" bgColor="GraphDark" path="symbols/current.xpm"/>
		<marquee x1="30" x2="-5" y1="0" y2="23" color="TextActiv" font="Sml">{MenuCurrent}</marquee>
	</list>

	<!-- Bottom: Message -->
	<block condition="{Message}">
		<rectangle x1="0" x2="-1" y1="-35" y2="-1" color="GraphDark"/>
		<rectangle x1="0" x2="-1" y1="-34" y2="-34" color="TextPassiv"/>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgInfo" align="center" font="Osd">{MessageInfo}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgStat" align="center" font="Osd">{MessageStatus}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgWarn" align="center" font="Osd">{MessageWarning}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgErr" align="center" font="Osd">{MessageError}</marquee>
	</block>
  
  </display>

<!-- Volume-Display ======================================= -->

  <display id="volume">
	<window x1="0" x2="-1" y1="-225" y2="-36" bpp="8"/>
	<window x1="0" x2="-1" y1="-35" y2="-1" bpp="8"/>

    <!-- Volume -->
	<block x1="100" x2="-100" y1="-136" y2="-100">
		<rectangle x1="0" x2="-1" y1="0" y2="-1" color="GraphDark"/>
		<ellipse x1="0" x2="4" y1="0" y2="4" color = "#00000000" arc="-2"/>
		<ellipse x1="-5" x2="-1" y1="0" y2="4" color = "#00000000" arc="-1"/>
		<ellipse x1="0" x2="4" y1="-5" y2="-1" color = "#00000000" arc="-3"/>
		<ellipse x1="-5" x2="-1" y1="-5" y2="-1" color = "#00000000" arc="-4"/>
		<rectangle x1="50" x2="-10" y1="7" y2="7" color="TextPassiv"/>
		<rectangle x1="50" x2="50" y1="7" y2="-8" color="TextPassiv"/>
		<rectangle x1="50" x2="-10" y1="-8" y2="-8" color="TextPassiv"/>
		<rectangle x1="-10" x2="-10" y1="7" y2="-8" color="TextPassiv"/>
		<progress x1="51" x2="-11" y1="8" y2="-9" color="TextActiv" current="{VolumeCurrent}" total="{VolumeTotal}"/>
		<image condition="not({VolumeIsMute})" bgColor="GraphDark" color="TextPassiv" x="10" y="10" path="symbols/volume.xpm"/>
		<image condition="{VolumeIsMute}" x="10" y="8" bgColor="GraphDark" color="TextPassiv" path="symbols/mute.xpm"/>
	</block>

	<!-- Bottom: Message -->
	<block condition="{Message}">
		<rectangle x1="0" x2="-1" y1="-35" y2="-1" color="GraphDark"/>
		<rectangle x1="0" x2="-1" y1="-34" y2="-34" color="TextPassiv"/>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgInfo" align="center" font="Osd">{MessageInfo}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgStat" align="center" font="Osd">{MessageStatus}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgWarn" align="center" font="Osd">{MessageWarning}</marquee>
		<marquee x1="5" x2="-5" y1="-28" y2="-1" color="MsgErr" align="center" font="Osd">{MessageError}</marquee>
	</block>

  </display>
  
