 <?xml version="1.0"?>
 <skin version="1.1" name="PearlHD" screenBase="relative" >
  
 <!--###############################################################################################
 #   VOLUME                                                                                        #
 ################################################################################################-->
 
  <display id="volume" >
 <!-- TOP WINDOW-->
   <window x1="1000" x2="-1" y1="0" y2="47" bpp="8" />
    <rectangle x1="1000" x2="-1" y1="0" y2="13" refresh="all" color="MainLight" />
    <ellipse   x1="1000" x2="1033" y1="13" y2="47" arc="3" refresh="all" color="MainLight" />
    <rectangle x1="1033" x2="-1" y1="13" y2="47" refresh="all" color="MainLight" />
    <text x1="1040" x2="-24" y1="13" y2="40" refresh="all" color="FontColor" bgColor="MainLight" align="left" condition="not({IsMute})" font="VDRSymbols Sans:Book@18" >Volume</text>
    <!-- trans('Volume') -->
    <text x1="1161" x2="-24" y1="13" y2="40" refresh="all,update" color="FontColor" bgColor="MainLight" align="right" condition="not({IsMute})" font="VDRSymbols Sans:Book@18" >{VolumeCurrent}</text>
    <text x1="1161" x2="-24" y1="13" y2="40" refresh="all,update" color="FontColor" bgColor="MainLight" align="right" condition="{IsMute}"    font="VDRSymbols Sans:Book@18" >Mute</text>
    <!-- trans('Mute') -->
 <!-- TOP WINDOW END-->
 <!-- BOTTOM WINDOW-->
    <window x1="0" x2="-1" y1="-26" y2="-1" bpp="8" />
     <progress   x1="0" x2="-1" y1="-26" y2="-1" bgColor="Bar" color="BarFill" current="{VolumeCurrent}" total="{VolumeTotal}" />
     <rectangle  x1="0" x2="-1" y1="-26" y2="-1" color="BarBlock" condition="{IsMute}" />
 <!-- BOTTOM WINDOW END-->
  </display>
  
 <!--###############################################################################################
 #   MESSAGE                                                                                       #
 ################################################################################################-->
 
  <display id="message" >
   <window x1="0" x2="-1" y1="-52" y2="-1" bpp="8" />
    <rectangle x1="83" x2="-1" y1="-46" y2="-12" refresh="all"  color="MainLight" />
    <ellipse    x1="50" x2="83" y1="-46" y2="-12" refresh="all"  arc="2" color="MainLight" />
    <rectangle  x1="50" x2="-1" y1="-12" y2="-1" refresh="all"  condition="{MessageInfo}" color="MainSolid" />
    <rectangle  x1="50" x2="-1" y1="-12" y2="-1" refresh="all"  condition="{MessageStatus}" color="Status" />
    <rectangle  x1="50" x2="-1" y1="-12" y2="-1" refresh="all"  condition="{MessageWarning}" color="Warning" />
    <rectangle  x1="50" x2="-1" y1="-12" y2="-1" refresh="all"  condition="{MessageError}" color="Error" />
    <text       x1="83" x2="-19" y1="-36" y2="-12" refresh="all"  color="FontColor" align="right" font="VDRSymbols Sans:Book@18" >{MessageInfo}</text>
    <text      x1="83" x2="-19" y1="-36" y2="-12" refresh="all"  color="FontColor" align="right" font="VDRSymbols Sans:Book@18" >{MessageStatus}</text>
    <text       x1="83" x2="-19" y1="-36" y2="-12" refresh="all"  color="FontColor" align="right" font="VDRSymbols Sans:Book@18" >{MessageWarning}</text>
    <text      x1="83" x2="-19" y1="-36" y2="-12" refresh="all"  color="FontColor" align="right" font="VDRSymbols Sans:Book@18" >{MessageError}</text>
  </display>
  
 <!--###############################################################################################
 #   REPLAY                                                                                        #
 ################################################################################################-->
  
  <display id="replayInfo" >
 <!-- TOP WINDOW-->
   <window x1="-239" x2="-1" y1="0" y2="47" bpp="8" />
    <rectangle refresh="all" x1="-239" x2="-1" y1="0" y2="13" color="MainLight" />
    
    <ellipse   refresh="all" x1="-239" x2="-206" y1="13" y2="47" arc="3" color="MainLight" />
    <rectangle refresh="all" x1="-206" x2="-1" y1="13" y2="47" color="MainLight" />
    
    <text condition="{IsPlaying}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Playing</text>
    <!-- trans('Playing') -->
    <text  condition="{IsPausing}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Pause</text>
    <!-- trans('Pause') -->
    <text    condition="{IsFastForward:0}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight"  font="VDRSymbols Sans:Book@18" >Fast forward</text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastForward:1}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Fast forward x1 </text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastForward:2}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Fast forward x2</text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastForward:3}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Fast forward x3</text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastRewind:0}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Fast rewind</text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsFastRewind:1}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Fast rewind x1 </text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsFastRewind:2}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Fast rewind x2</text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsFastRewind:3}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Fast rewind x3</text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsSlowForward:0}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow forward</text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowForward:1}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow forward x1 </text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowForward:2}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow forward x2</text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowForward:3}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow forward x3</text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowRewind:0}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow rewind</text>
    <!-- trans('Slow rewind') -->
    <text    condition="{IsSlowRewind:1}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow rewind x1 </text>
    <!-- trans('Slow rewind') -->
    <text    condition="{IsSlowRewind:2}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow rewind x2</text>
    <!-- trans('Slow rewind') -->
    <text    condition="{IsSlowRewind:3}" x1="-206" x2="-12" y1="13" y2="40" align="right" color="FontColor" bgColor="MainLight" font="VDRSymbols Sans:Book@18" >Slow rewind x3</text>
    <!-- trans('Slow rewind') -->
 <!-- TOP WINDOW END-->
        
 <!-- BOTTOM WINDOW-->
   <window x1="0" x2="-1" y1="-106" y2="-1" bpp="8" />
    <ellipse  x1="0" x2="33" y1="-72" y2="-39" arc="2" color="MainLight" />
    <rectangle x1="34" x2="-1" y1="-72" y2="-39" color="MainLight" />
    <rectangle x1="0" x2="-1" y1="-39" y2="-26" color="MainSolid" />
    <text x1="160" x2="-212" y1="-62" y2="-26" color="FontColor" font="VDRSymbols Sans:Book@18" >{ReplayTitle}</text>
    <text x1="160" x2="-19" y1="-62" y2="-26" color="FontColor" align="right" font="VDRSymbols Sans:Book@18" >{ReplayPosition} of {ReplayDuration}</text>
    <!-- trans('of') -->
    <progress x1="0" x2="-1" y1="-26" y2="-1" color="BarFill" mark="Mark" keep="Keep" bgColor="Bar" current="{ReplayPositionIndex}" total="{ReplayDurationIndex}" />
    <ellipse x1="-239" x2="-206" y1="-106" y2="-73" arc="2" condition="{ReplayPrompt}" color="MainLight" />
    <rectangle x1="-206" x2="-1" y1="-106" y2="-73"  condition="{ReplayPrompt}" color="MainLight" />
    <text x1="-166" x2="-19" y1="-99" y2="-73" color="FontColor" align="right" font="VDRSymbols Sans:Book@18" >{ReplayPrompt}</text>
 <!-- BOTTOM WINDOW END-->
  </display>
   
 <!--###############################################################################################
 #   CHANNEL INFO                                                                                  #
 ################################################################################################-->
  
  <display id="channelInfo" >
 <!-- TOP WINDOW-->
   <window x1="-306" x2="-1" y1="0" y2="47" bpp="8" />
    <rectangle x1="-306" x2="-1" y1="0" y2="13" color="MainLight" />
    <ellipse  x1="-306" x2="-272" y1="13" y2="47" arc="3" color="MainLight" />
    <rectangle x1="-272" x2="-1" y1="13" y2="47" color="MainLight" />
    <text x1="-286" x2="-12" y1="13" y2="40" color="FontColor" align="right" font="VDRSymbols Sans:Book@18" >{DateTime:%A %e.%m. %H\:%M}</text>
 <!-- TOP WINDOW END--> 
 
 <!-- LOWER WINDOW-->
   <window x1="160" x2="-159" y1="-226" y2="-12" bpp="8" />
 <!-- HEAD -->   
    <block>
     <ellipse x1="160" x2="193" y1="-226" y2="-192" arc="2" color="MainLight" />
     <rectangle x1="193" x2="-159" y1="-226" y2="-192" color="MainLight" />
     <rectangle x1="160" x2="-159" y1="-192" y2="-179" color="Bar" />
     <progress x1="160" x2="-159" y1="-192" y2="-179" color="BarFill" bgColor="Bar" current="{PresentProgress}" total="{PresentDuration}" />
    </block>
 <!-- HEAD END-->
    
 <!-- BODY -->
 <!-- SYMBOLS --> 
    <block>
     <rectangle x1="160" x2="-159" y1="-179" y2="-36" color="MainSolid" />
 
 <!-- NOCHANNELLOGO -->    <text x1="240" x2="320" y1="-217" y2="-166" color="FontColor" font="VDRSymbols Sans:Book@18" >{ChannelNumber}</text>
 <!-- NOCHANNELLOGO -->    <text x1="320" x2="-306" y1="-222" y2="-166" color="FontColor" font="VDRSymbols Sans:Book@24" >{ChannelName}</text>
     <block x1="-296" x2="-322" y1="-216" y2="-229" condition="not({HasTeletext})" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Divider" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      <rectangle x1="3" x2="13" y1="3" y2="5" color="Divider" />
      <rectangle x1="3" x2="13" y1="6" y2="7" color="Divider" />
      <rectangle x1="3" x2="13" y1="9" y2="10" color="Divider" />
     </block>
     <block x1="-296" x2="-322" y1="-216" y2="-229" condition="{HasTeletext}" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Green" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      <rectangle x1="3" x2="13" y1="3" y2="5" color="Green" />
      <rectangle x1="3" x2="13" y1="6" y2="7" color="Green" />
      <rectangle x1="3" x2="13" y1="9" y2="10" color="Green" />
     </block>
     <block x1="-269" x2="-296" y1="-216" y2="-229" condition="not({HasDolby})" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Divider" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      <rectangle x1="8" x2="9" y1="2" y2="11" color="Divider" />
      <ellipse x1="3" x2="7" y1="3" y2="10" arc="5"  color="Divider" />
      <ellipse x1="10" x2="13" y1="3" y2="10" arc="7"  color="Divider" />
     </block>
     <block x1="-269" x2="-296" y1="-216" y2="-229" condition="{HasDolby}" >
      <block >
       <rectangle x1="1" x2="15" y1="1" y2="12" color="Yellow" />
       <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
       <rectangle x1="8" x2="9" y1="2" y2="11" color="Yellow" />
       <ellipse x1="3" x2="7" y1="3" y2="10" arc="5"  color="Yellow" />
       <ellipse x1="10" x2="13" y1="3" y2="10" arc="7"  color="Yellow" />
      </block> 
      <block condition="equal({Language},'deuDD')" >
       <rectangle x1="1" x2="15" y1="1" y2="12" color="Green" />
       <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
       <rectangle x1="8" x2="9" y1="2" y2="11" color="Green" />
       <ellipse x1="3" x2="7" y1="3" y2="10" arc="5"  color="Green" />
       <ellipse x1="10" x2="13" y1="3" y2="10" arc="7"  color="Green" />
      </block>
      <block condition="equal({Language},'ddDD')" >
       <rectangle x1="1" x2="15" y1="1" y2="12" color="Green" />
       <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
       <rectangle x1="8" x2="9" y1="2" y2="11" color="Green" />
       <ellipse x1="3" x2="7" y1="3" y2="10" arc="5"  color="Green" />
       <ellipse x1="10" x2="13" y1="3" y2="10" arc="7"  color="Green" />
      </block>
      
     </block>
     <block x1="-242" x2="-269" y1="-216" y2="-229" condition="not({HasMultilang})" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Divider" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      <ellipse x1="3" x2="9" y1="4" y2="9" color="Divider" />
      <ellipse x1="4" x2="8" y1="5" y2="9" color="Divider" />
      <ellipse x1="8" x2="13" y1="4" y2="9" color="Divider" />
      <ellipse x1="9" x2="13" y1="5" y2="9" color="MainLight" />
     </block>
     <block x1="-242" x2="-269" y1="-216" y2="-229" condition="{HasMultilang}" >
      
     
     
      <block condition="equal({AudioChannel},'stereo')" >
       <rectangle x1="1" x2="15" y1="1" y2="12" color="Green" />
       <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
       <ellipse x1="3" x2="9" y1="4" y2="9" color="Green" />
       <ellipse x1="4" x2="9" y1="5" y2="9" color="Green" />
       <ellipse x1="8" x2="13" y1="4" y2="9" color="Green" />
       <ellipse x1="9" x2="13" y1="5" y2="9" color="Green" />
      </block>
      <block condition="equal({AudioChannel},'left')" >
       <rectangle x1="1" x2="15" y1="1" y2="12" color="Green" />
       <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
       <ellipse x1="3" x2="9" y1="4" y2="9" color="Green" />
       <ellipse x1="4" x2="9" y1="5" y2="9" color="Green" />
       <ellipse x1="8" x2="13" y1="4" y2="9" color="Green" />
       <ellipse x1="9" x2="13" y1="5" y2="9" color="MainLight" />
      </block>
      <block condition="equal({AudioChannel},'right')" >
       <rectangle x1="1" x2="15" y1="1" y2="12" color="Green" />
       <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
       <ellipse x1="3" x2="9" y1="4" y2="9" color="Green" />
       <ellipse x1="4" x2="9" y1="5" y2="9" color="MainLight" />
       <ellipse x1="8" x2="13" y1="4" y2="9" color="Green" />
       <ellipse x1="9" x2="13" y1="5" y2="9" color="Green" />
      </block>
      
     </block>
     <block x1="-216" x2="-242" y1="-216" y2="-229" condition="not({IsEncrypted})" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Divider" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      <ellipse x1="9" x2="14" y1="3" y2="10" color="Divider" />
      <ellipse x1="10" x2="13" y1="5" y2="9" color="MainLight" />
      <rectangle x1="3" x2="9" y1="6" y2="7" color="Divider" />
      <rectangle x1="3" x2="4" y1="7" y2="9" color="Divider" />
      <rectangle x1="5" x2="6" y1="7" y2="8" color="Divider" />
     </block>
     <block x1="-216" x2="-242" y1="-216" y2="-229" condition="{IsEncrypted}" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Green" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      <ellipse x1="9" x2="14" y1="3" y2="10" color="Green" />
      <ellipse x1="10" x2="13" y1="5" y2="9" color="MainLight" />
      <rectangle x1="3" x2="9" y1="6" y2="7" color="Green" />
      <rectangle x1="3" x2="4" y1="7" y2="9" color="Green" />
      <rectangle x1="5" x2="6" y1="7" y2="8" color="Green" />
     </block>
     <block x1="-189" x2="-216" y1="-216" y2="-229" condition="not({IsRecording})" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Divider" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      <blink x1="3" x2="17" y1="3" y2="13" color="Warning" delay="400" condition="{TimerConflicts}"  font="VDRSymbols Sans:Bold@6" >rec</blink>
      <text x1="3" x2="17" y1="3" y2="13" color="Divider" condition="not({TimerConflicts})"  font="VDRSymbols Sans:Bold@6" >rec</text>
     </block>
     <block x1="-189" x2="-216" y1="-216" y2="-229" condition="{IsRecording}" >
      <rectangle x1="1" x2="15" y1="1" y2="12" color="Red" />
      <rectangle x1="2" x2="15" y1="2" y2="11" color="MainLight" />
      
      <blink x1="3" x2="17" y1="3" y2="13" color="Warning" delay="800" condition="{TimerConflicts}"  font="VDRSymbols Sans:Bold@6" >rec</blink>
      <text x1="3" x2="17" y1="3" y2="13" color="Red" condition="not({TimerConflicts})" font="VDRSymbols Sans:Bold@6" >rec</text>
     </block>
 <!-- SYMBOLS END --> 
 
 <!-- PRESENT-->
     <text x1="240" x2="320" y1="-160" y2="-136" color="FontColor" font="VDRSymbols Sans:Book@18" >{PresentStartDateTime:%H\:%M} </text>
     <text x1="320" x2="-239" y1="-166" y2="-136" color="FontColor" font="VDRSymbols Sans:Book@24" condition="not({HasTimer})" >{PresentTitle}</text>
     <text x1="320" x2="-239" y1="-166" y2="-136" color="FontColor" font="VDRSymbols Sans:Book@24" condition="{HasTimer}" >{PresentTitle} *</text>
 <!-- NORCHANNELLOGO -->    <text x1="-239" x2="-189" y1="-162" y2="-136" color="FontColor" align="right" font="VDRSymbols Sans:Book@18" condition="{PresentRemaining}" >{PresentRemaining:%m}</text>
 <!-- NORCHANNELLOGO -->    <text x1="-239" x2="-176" y1="-162" y2="-136" color="FontColor" align="right" font="VDRSymbols Sans:Book@10" condition="{PresentRemaining}" >m</text>
     <text x1="320" x2="-239" y1="-136" y2="-96" color="FontColor" font="VDRSymbols Sans:Book@18" >{PresentShortText}</text>
 <!-- PRESENT END-->
 <!-- NEXT  -->
     <text x1="240" x2="320" y1="-97" y2="-72" color="FontColor" font="VDRSymbols Sans:Book@18" >{FollowingStartDateTime:%H\:%M}</text>
     <text x1="320" x2="-239" y1="-102" y2="-72" color="FontColor" font="VDRSymbols Sans:Book@24" condition="not({FollowingHasTimer})" >{FollowingTitle}</text>
     <text x1="320" x2="-239" y1="-102" y2="-72" color="FontColor" font="VDRSymbols Sans:Book@24" condition="{FollowingHasTimer}" >{FollowingTitle} *</text>
 <!-- NORCHANNELLOGO -->    <text x1="-239" x2="-189" y1="-99" y2="-72" color="FontColor" align="right" font="VDRSymbols Sans:Book@18" condition="{FollowingDuration}" >{FollowingDuration:%m} </text>
 <!-- NORCHANNELLOGO -->    <text x1="-239" x2="-176" y1="-99" y2="-72" color="FontColor" align="right" font="VDRSymbols Sans:Book@10" condition="{FollowingDuration}" >m</text>
     <text x1="320" x2="-239" y1="-72" y2="-12" color="FontColor" font="VDRSymbols Sans:Book@18" >{FollowingShortText}</text>
 <!-- NEXT END  -->
     
    </block>
 <!-- BODY END -->
 <!-- BUTTONS-->
    <block refresh="all,update,full" changed="{ButtonRed}{ButtonGreen}{ButtonYellow}{ButtonBlue}" >
     <block x1="-797" x2="-157" y1="-36" y2="-12" >
